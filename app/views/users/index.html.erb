<% provide(:title, 'All users') %>
<h1>All users</h1>


<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
	<% name = [user.firstName, user.lastName].join(' ')%>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to name, user %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>
